{% extends 'base.html.twig' %}

{% block title %}CategoryFood{% endblock %}

{% block body %}
    <h1>CategoryFood</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category_food.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ category_food.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ category_food.createdAt ? category_food.createdAt|date('Y-m-d H:i') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ category_food.updatedAt ? category_food.updatedAt|date('Y-m-d H:i') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_office_category_food_index') }}"class="btn btn-primary"><i class="fas fa-undo-alt"></i></a>

    <a href="{{ path('back_office_category_food_edit', {'id': category_food.id}) }}"class="btn btn-warning mx-2"><i class="far fa-edit"></i></a>

    {{ include('back_office/category_food/_delete_form.html.twig') }}
{% endblock %}
